{
  "version": 2,
  "functions": {
    "api/server.js": {
      "includeFiles": ".sls/**/*.js"
    }
  },
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "(.*)",
      "dest": "/api/server"
    }
  ]
}
